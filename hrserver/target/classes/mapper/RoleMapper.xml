<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.ncvt.mapper.RoleMapper">
    <select id="roles" resultType="role">
        SELECT * FROM role where id!=6;
    </select>
    <insert id="addNewRole" parameterType="String">
        INSERT INTO role set name=#{role},nameZh=#{roleZh}
    </insert>
    <delete id="deleteRoleById" parameterType="int">
        DELETE FROM role WHERE id=#{rid}
    </delete>
</mapper>